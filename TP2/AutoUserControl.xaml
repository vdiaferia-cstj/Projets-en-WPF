<UserControl x:Class="TP2.AutoUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TP2"
             mc:Ignorable="d" 
             d:DesignHeight="250" d:DesignWidth="200">
    <StackPanel Background="#FF2F2B2B" >
        
        <TextBlock x:Name="AutoImage" Height="150" Width="150" Background="#FF2F2B2B" Foreground="White" />
        <WrapPanel>
            <TextBlock x:Name="AutoPrice"></TextBlock>
            <TextBlock x:Name="AutoPublishDate"></TextBlock>

        </WrapPanel>
        <WrapPanel>
        <TextBlock x:Name="AutoBrand"></TextBlock>
         <TextBlock x:Name="AutoModel"></TextBlock>

        </WrapPanel>
    </StackPanel>
</UserControl>

<Window x:Class="TP2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TP2"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="250">
    <Grid Background="Black">
        <Grid.RowDefinitions>
            <RowDefinition ></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        
        <TextBlock Grid.Row="0"  Height="60" Background="DeepSkyBlue">
        <Button Click="Wall_Button" Background="#FF3A69C7">
        Wall
        </Button></TextBlock>
        
        <TextBlock Height="60" Grid.Row="1" Background="DeepSkyBlue">
            <Button Background="#FF3A69C7"  >
                Marketplace
            </Button>
        </TextBlock>

        
    </Grid>


</Window>

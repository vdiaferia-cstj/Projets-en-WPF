<Window x:Class="TP2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TP2"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="250">
    <Grid Background="Black">
        <Grid.RowDefinitions>
            <RowDefinition ></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>


        <Button Foreground="White" Margin="0,20,0,20" Click="Wall_Button" Background="#FF3A69C7">
            Wall
        </Button>


        <Button Foreground="White" Margin="0,20,0,20" Grid.Row="1" Click="Button_Click" Background="#FF3A69C7">
            Marketplace
        </Button>



    </Grid>


</Window>
